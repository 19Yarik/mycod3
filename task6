class ListNode:
    def __init__(self, val=0, next=None):
        self.val = val
        self.next = next

def doubleNumber(head: ListNode) -> ListNode:
    # 1. Перетворюємо зв'язаний список у число
    current = head
    num = 0
    while current:
        num = num * 10 + current.val
        current = current.next
    
    # 2. Подвоюємо число
    num *= 2
    
    # 3. Створюємо новий зв'язаний список з подвоєного числа
    dummy = ListNode(0)
    current = dummy
    if num == 0:
        return ListNode(0)  # на випадок, якщо число 0 (не має бути за умовою)
    
    for digit in str(num):
        current.next = ListNode(int(digit))
        current = current.next
    
    return dummy.next

# Допоміжна функція для виведення списку
def print_list(node):
    while node:
        print(node.val, end=" -> ")
        node = node.next
    print("None")

# Тестування
head1 = ListNode(1, ListNode(8, ListNode(9)))  # 189
head2 = ListNode(9, ListNode(9, ListNode(9)))   # 999

# Подвоєння чисел
result1 = doubleNumber(head1)
result2 = doubleNumber(head2)

# Виводимо результати
print_list(result1)  # Output: 3 -> 7 -> 8 -> None
print_list(result2)  # Output: 1 -> 9 -> 9 -> 8 -> None
